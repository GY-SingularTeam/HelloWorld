#include<stdio.h>
#include<stdlib.h>
#include<string.h>
char a[1234567];
char b[1234567];
char c[1234567];
void  fun(char b[],int n)
{
//	puts(b);
//	printf("\nn=%d\n",n);
//	system("pause");
	int k=0,i;
	for(i=n-1;i>=0;i-=3)
	{
		if(i-2>=0)
		{
		//	printf("A\n");
			if(b[i-2]=='0'&&b[i-1]=='0'&&b[i]=='0')
			{
				c[k++]='0';
			}
			else if(b[i-2]=='0'&&b[i-1]=='0'&&b[i]=='1')
			{
				c[k++]='1';
			}
			else if(b[i-2]=='0'&&b[i-1]=='1'&&b[i]=='0')
			{
					c[k++]='2';
			}
				else if(b[i-2]=='0'&&b[i-1]=='1'&&b[i]=='1')
			{
					c[k++]='3';
			}
				else if(b[i-2]=='1'&&b[i-1]=='0'&&b[i]=='0')
			{
					c[k++]='4';
			}
				else if(b[i-2]=='1'&&b[i-1]=='0'&&b[i]=='1')
			{
					c[k++]='5';
			}
				else if(b[i-2]=='1'&&b[i-1]=='1'&&b[i]=='0')
			{
					c[k++]='6';
			}
				else if(b[i-2]=='1'&&b[i-1]=='1'&&b[i]=='1')
			{
					c[k++]='7';
			}
			//	printf("%c\n",c[k-1]);
		}
		else 
		{
		//	printf("AA\n");
			if(i=0)
			{
				if(b[0]=='0')
				{
					c[k++]='0';
				}
				else if(b[0]=='1')
				{
					c[k++]='1';
				}
				break;
			}
			else if(i=1)
			{
				 if(b[0]=='0'&&b[1]=='0')
				 {
				 	c[k++]='0';
				 }
				 if(b[0]=='0'&&b[1]=='1')
				{
					c[k++]='1';
				}
				else if(b[0]=='1'&&b[1]=='0')
				{
					c[k++]='2';
				}
				else if(b[0]=='1'&&b[1]=='1')
				{
					c[k++]='3';
				}
					break;
			}
		 } 
	}
//	printf("k=%d\n",k);
	for( i=k-1;i>=0;i--)
	{
		if(i==k-1)
		{
			if(c[i]!='0')
			{
				printf("%c",c[i]);
			}
		}
		else
		{
			if(c[i+1]!=0)
			{
				printf("%c",c[i]);
			}
		}
	}
	printf("\n");
}
int main()
{
	int n,len,l=0,i;
	scanf("%d",&n);
	getchar();
	while(n--)
	{
		l=0;
		scanf("%s",a);
	//	puts(a);S
		len=strlen(a);
		for(i=0;i<len;i++)
		{
			switch(a[i])
		{
			case '0':b[l++]='0',b[l++]='0',b[l++]='0',b[l++]='0';break;
			case '1':b[l++]='0',b[l++]='0',b[l++]='0',b[l++]='1';break;
			case '2':b[l++]='0',b[l++]='0',b[l++]='1',b[l++]='0';break;
			case '3':b[l++]='0',b[l++]='0',b[l++]='1',b[l++]='1';break;
			case '4':b[l++]='0',b[l++]='1',b[l++]='0',b[l++]='0';break;
			case '5':b[l++]='0',b[l++]='1',b[l++]='0',b[l++]='1';break;
			case '6':b[l++]='0',b[l++]='1',b[l++]='1',b[l++]='0';break;
			case '7':b[l++]='0',b[l++]='1',b[l++]='1',b[l++]='1';break;
			case '8':b[l++]='1',b[l++]='0',b[l++]='0',b[l++]='0';break;
			case '9':b[l++]='1',b[l++]='0',b[l++]='0',b[l++]='1';break;
			case 'A':b[l++]='1',b[l++]='0',b[l++]='1',b[l++]='0';break;
			case 'B':b[l++]='1',b[l++]='0',b[l++]='1',b[l++]='1';break;
			case 'C':b[l++]='1',b[l++]='1',b[l++]='0',b[l++]='0';break;
			case 'D':b[l++]='1',b[l++]='1',b[l++]='0',b[l++]='1';break;
			case 'E':b[l++]='1',b[l++]='1',b[l++]='1',b[l++]='0';break;
			case 'F':b[l++]='1',b[l++]='1',b[l++]='1',b[l++]='1';break;
		}
		 } 
		 b[l]='\0';
	//	 puts(b);
//		for(int i=0;i<l;i++)
//		{
//			printf("%c",b[i]);
//		}
		 fun(b,l);
	}
}
