import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
         Scanner lnt =new Scanner(System.in);
         int n=lnt.nextInt();
         int m=lnt.nextInt();
         char []a=new char[26];
         for(int i=0;i<26;i++) {
        	 a[i]=(char)('A'+i);
         }
         for(int i=0;i<n;i++) {
        	 for(int j=0;j<m;j++) {
        		 System.out.print(a[Math.abs(j-i)]);
        	 }
        	 System.out.println();
         }
	}

}
