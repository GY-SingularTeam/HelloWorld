package text_1_1;

import java.math.BigInteger;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner lnt = new Scanner(System.in);
		int n = lnt.nextInt();
		for (int i = 0; i < n; i++) {
			String s = lnt.next();
			BigInteger a = new BigInteger(s, 16);
			System.out.println(a.toString(8));
		}
	}

}
